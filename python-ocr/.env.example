# Azure Document Intelligence Configuration
AZURE_DOCUMENT_INTELLIGENCE_ENDPOINT=https://your-resource.cognitiveservices.azure.com/
AZURE_DOCUMENT_INTELLIGENCE_KEY=your-document-intelligence-key

# API Configuration
API_PREFIX=/api/v1

# CORS Settings (customize for production)
CORS_ORIGINS=["http://localhost:3000"]
CORS_HEADERS=["*"]
CORS_METHODS=["*"]
# Debug Configuration
# Set to 'true' to enable image processing debug mode (saves images at each stage)
DEBUG_IMAGE_PROCESSING=false
# Set to 'true' to enable Tesseract OCR debug mode
DEBUG_TESSERACT=false
# Directory for debug output files (images, text files)
DEBUG_OUTPUT_DIR=debug_ocr
